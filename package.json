{
  "name": "itemslide",
  "version": "1.6.0",
  "description": "A simple and beautiful JavaScript touch carousel",
  "main": "dist/itemslide.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "start ./index.html",
    "release-build": "echo /* ItemSlide.js - Licensed under the MIT license - itemslide.org/license.html */ > dist/itemslide.min.js && browserify src/itemslide.js | uglifyjs >> dist/itemslide.min.js",
    "testing-build": "browserify src/itemslide.js > dist/itemslide.js",
    "watch": "watchify src/itemslide.js -o dist/itemslide.js",
    "test": "start ./testing/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nir9/itemslide"
  },
  "keywords": [
    "jquery-plugin",
    "carousel",
    "touch",
    "mobile",
    "slide",
    "swipe"
  ],
  "homepage": "https://itemslide.org",
  "author": "Nir Lichtman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nir9/itemslide/issues"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "uglify-js": "^3.10.0",
    "watchify": "^3.11.1"
  }
}
